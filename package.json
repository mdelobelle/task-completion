{
	"name": "task-completion",
	"version": "0.0.1",
	"description": "Insert task toggler inline",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"css:clean": "rm -f styles.css",
		"css:compile": "sass ./assets/css/styles.scss ./styles.css --no-source-map",
		"build:css": "npm run css:clean && npm run css:compile",
		"build:css:watch": "onchange -i './assets/css/*.scss' './assets/css/*.css' -- npm run build:css"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@codemirror/language": "github:lishid/cm-language",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"onchange": "^7.1.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"sass": "^1.66.0",
		"tippy.js": "^6.3.7"
	}
}